{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 112",
    "not chrome <= 112",
    "not edge <= 112",
    "not firefox <= 112",
    "not safari <= 16.4",
    "not samsung <= 20"
  ]
}